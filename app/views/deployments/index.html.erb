<p id="notice"><%= notice %></p>

<h1>Deployments</h1>

<table>
  <thead>
    <tr>
      <th>Deploy notes</th>
      <th>Deploy date</th>
      <th>Deploy time</th>
      <th>Retrieve status</th>
      <th>Retrieve notes</th>
      <th>Retrieve date</th>
      <th>Retrieve time</th>
      <th>Site</th>
      <th>Unit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deployments.each do |deployment| %>
      <tr>
        <td><%= deployment.deploy_notes %></td>
        <td><%= deployment.deploy_date %></td>
        <td><%= deployment.deploy_time %></td>
        <td><%= deployment.retrieve_status %></td>
        <td><%= deployment.retrieve_notes %></td>
        <td><%= deployment.retrieve_date %></td>
        <td><%= deployment.retrieve_time %></td>
        <td><%= deployment.site %></td>
        <td><%= deployment.unit %></td>
        <td><%= link_to 'Show', deployment %></td>
        <td><%= link_to 'Edit', edit_deployment_path(deployment) %></td>
        <td><%= link_to 'Destroy', deployment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deployment', new_deployment_path %>
